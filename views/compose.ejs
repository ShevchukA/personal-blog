<%- include('partials/header') %>
<h1>Compose</h1>
<form action="/compose" method="post">
    <div class="mb-3">
        <label class="form-label" for="postTitle">Title</label>
        <input class="form-control" type="text" name="postTitle" autocomplete="off">
    </div>
    <div class="mb-3">
        <label class="form-label" for="postText">Post</label>
        <textarea class="form-control" name="postText" rows="10"></textarea>
    </div>
    <button class="btn btn-info" type="submit" name="submitButton">Publish</button>
</form>
<%- include('partials/footer') %>